{
  "name": "united-states-of-america-united",
  "version": "1.0.0",
  "description": "An alternate history of American isolationism and innovation - a branching narrative storybook",
  "main": "index.js",
  "scripts": {
    "dev": "npx @11ty/eleventy --serve --watch",
    "build": "NODE_ENV=production npx @11ty/eleventy",
    "css:dev": "npx tailwindcss -i ./src/assets/css/main.css -o ./_site/assets/css/styles.css --watch",
    "css:build": "npx tailwindcss -i ./src/assets/css/main.css -o ./_site/assets/css/styles.css --minify",
    "start": "npm-run-all --parallel dev css:dev",
    "deploy": "npm run css:build && npm run build"
  },
  "keywords": [
    "alternate-history",
    "storybook",
    "branching-narrative",
    "america",
    "eleventy",
    "static-site"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "alpinejs": "^3.13.3",
    "lunr": "^2.3.9",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6"
  }
}
